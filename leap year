year= eval(input("enter a year:"))
if year <=0 or year ==float:
    print("invalid")

if year%400==0:
    isleapyear= True
elif year%100==0:
    isleapyear= False
elif year%4==0:
    isleapyear= True
else:
    isleapyear = False
if isleapyear:
    print("it is an leap year")
else:
    print("it is not leap year")
    m=year%4
    print("previous leap year is",year-m)
